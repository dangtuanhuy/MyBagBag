<body>
    <!-- container section start -->
    <section id="container" class="">


        <header class="header dark-bg">
            <div class="toggle-nav">
                <div class="icon-reorder tooltips" data-original-title="Toggle Navigation" data-placement="bottom"><i class="icon_menu"></i></div>
            </div>

            <!--logo start-->
            <a href="@Url.Action("Index","Page")" class="logo">Bag <span class="lite">Bag</span></a>
            <!--logo end-->

            <div class="nav search-row" id="top_menu">
                <!--  search form start -->
                <ul class="nav top-menu">
                    <li>
                        <form class="navbar-form">
                            <input class="form-control" placeholder="Search" type="text">
                        </form>
                    </li>
                </ul>
                <!--  search form end -->
            </div>

            <div class="top-nav notification-row">
                <!-- notificatoin dropdown start-->
                <ul class="nav pull-right top-menu">



                    <li class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                            <span class="profile-ava">
                                <img alt="" src="~/Content/Admin/img/avatar1_small.jpg">
                            </span>
                            <span class="username">@Session["employee"]</span>
                            <b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu extended logout">
                            <div class="log-arrow-up"></div>
                            <li class="eborder-top">
                                <a href="#"><i class="icon_profile"></i> My Profile</a>
                            </li>
                            <li>
                                <a href="#"><i class="icon_mail_alt"></i>Update Password</a>
                            </li>
                            <li>
                                <a href="@Url.Action("Login","Login")"><i class="icon_key_alt"></i> Log Out</a>
                            </li>

                        </ul>
                    </li>
                    <!-- user login dropdown end -->
                </ul>
                <!-- notificatoin dropdown end-->
            </div>
        </header>
        <!--header end-->
        <!--sidebar start-->
        <aside>
            <div id="sidebar" class="nav-collapse ">
                <!-- sidebar menu start-->
                <ul class="sidebar-menu">
                    <li class="active">
                        <a class="" href="@Url.Action("Index","Page")">
                            <i class="icon_house_alt"></i>
                            <span>Home</span>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("Index","Abouts")" class="">
                            <i class="fa fa-address-book"></i>
                            <span>Abouts</span>

                        </a>
                    </li>
                    <li class="sub-menu">
                        <a href="javascript:;" class="">
                            <i class="fa fa-meetup"></i>
                            <span>Product</span>
                            <span class="menu-arrow arrow_carrot-right"></span>
                        </a>
                        <ul class="sub">
                            <li><a class="" href="@Url.Action("Index","Categories")">Category</a></li>
                            <li><a class="" href="@Url.Action("Index","Suppliers")">Suppliers</a></li>
                            <li><a class="" href="@Url.Action("Index","Promotions")">Promotion</a></li>
                            <li><a class="" href="@Url.Action("Index","Products")">Products</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="@Url.Action("Index","Deliveries")" class="">
                            <i class="fa fa-bus"></i>
                            <span>Deliveries</span>

                        </a>
                    </li>
                    <li>
                        <a class="" href="@Url.Action("Index","Branches")">
                            <i class="fa fa-gg"></i>
                            <span>Branches</span>
                        </a>
                    </li>
                    <li>
                        <a class="" href="@Url.Action("Index","News")">
                            <i class="fa fa-newspaper-o"></i>
                            <span>News</span>

                        </a>

                    </li>

                    <li class="sub-menu">
                        <a href="javascript:;" class="">
                            <i class="fa fa-users"></i>
                            <span>Employee</span>
                            <span class="menu-arrow arrow_carrot-right"></span>
                        </a>
                        <ul class="sub">
                            <li><a class="" href="@Url.Action("Index","Customers")">Customer</a></li>
                            <li><a class="" href="@Url.Action("Index","Employees")">Employees</a></li>
                            <li><a class="" href="@Url.Action("Index","Orders")">Order</a></li>
                            <li><a class="" href="@Url.Action("Index","PaymentMethods")">Payment</a></li>
                            <li><a class="" href="@Url.Action("Index","Roles")">Role</a></li>
                        </ul>
                    </li>

                    <li class="sub-menu">
                        <a href="@Url.Action("Index","Contacts")" class="">
                            <i class="fa fa-sitemap"></i>
                            <span>Contacts</span>

                        </a>

                    </li>

                </ul>
                <!-- sidebar menu end-->
            </div>
        </aside>
        <!--sidebar end-->
        <!--main content start-->
        <section id="main-content">
            <section class="wrapper">
                <!--overview start-->
                <div class="row">
                    <div class="col-lg-12">
                        <h3 class="page-header"><i class="fa fa-laptop"></i> Directional</h3>
                        <ol class="breadcrumb">
                            <li><i class="fa fa-home"></i><a href="@Url.Action("Index","Page")">Main</a></li>
                            <li><i class="fa fa-laptop"></i>Home</li>
                        </ol>
                    </div>
                </div>

                <div class="row">
                    <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">

                        @Html.Action("_countContacts", "Page")
                    </div>
                    <!--/.col-->

                    <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
                        @Html.Action("_countCart", "Page")
                        <!--/.info-box-->
                    </div>
                    <!--/.col-->

                    <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
                        @Html.Action("_countProducts", "Page")
                        <!--/.info-box-->
                    </div>
                    <!--/.col-->

                    <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
                        @Html.Action("_statusPromotion", "Page")
                        <!--/.info-box-->
                    </div>
                    <!--/.col-->

                </div>
                <!--/.row-->

                @RenderBody()





            </section>
            <div class="text-center">
                @{ Html.RenderAction("_FooterPage", "Page");}
            </div>
        </section>
        <!--main content end-->
    </section>
    <!-- container section start -->
    <!-- javascripts -->
    <script src="~/Content/Admin/js/jquery.js"></script>
    <script src="~/Content/Admin/js/jquery-ui-1.10.4.min.js"></script>
    <script src="~/Content/Admin/js/jquery-1.8.3.min.js"></script>
    <script type="text/javascript" src="~/Content/Admin/js/jquery-ui-1.9.2.custom.min.js"></script>

    <!-- bootstrap -->
    <script src="~/Content/Admin/js/bootstrap.min.js"></script>

    <link href="~/Content/Admin/assets/datatables/datatables.min.css" rel="stylesheet" />
    <script src="~/Content/Admin/assets/datatables/datatables.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $('#tablefillset').DataTable(
                {
                    "language": {
                        "infoEmpty": "Data null",
                        "emptyTable": "Null Data",
                        "processing": "Access...",
                        "search": "Search:",
                        "loadingRecords": "load...",
                        "zeroRecords": "Data cannot find",
                        "paginate": {
                            "first": "|<",
                            "last": ">|",
                            "next": ">>",
                            "previous": "<<"
                        }
                    },
                    "lengthMenu": [[5, 10, 15, 20, 25, 30, -1], [5, 10, 15, 20, 25, 30, "All"]]
                }

            );


        });
    </script>
    <!-- nice scroll -->
    <script src="~/Content/Admin/js/jquery.scrollTo.min.js"></script>
    <script src="~/Content/Admin/js/jquery.nicescroll.js" type="text/javascript"></script>
    <!-- charts scripts -->
    <script src="~/Content/Admin/assets/jquery-knob/js/jquery.knob.js"></script>
    <script src="~/Content/Admin/js/jquery.sparkline.js" type="text/javascript"></script>
    <script src="~/Content/Admin/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.js"></script>
    <script src="~/Content/Admin/js/owl.carousel.js"></script>
    <!-- jQuery full calendar -->
    <
    <script src="~/Content/Admin/js/fullcalendar.min.js"></script>
    <!-- Full Google Calendar - Calendar -->
    <script src="~/Content/Admin/assets/fullcalendar/fullcalendar/fullcalendar.js"></script>
    <!--script for this page only-->
    <script src="~/Content/Admin/js/calendar-custom.js"></script>
    <script src="~/Content/Admin/js/jquery.rateit.min.js"></script>
    <!-- custom select -->
    <script src="~/Content/Admin/js/jquery.customSelect.min.js"></script>
    <script src="~/Content/Admin/assets/chart-master/Chart.js"></script>

    <!--custome script for all page-->
    <script src="~/Content/Admin/js/scripts.js"></script>
    <!-- custom script for this page-->
    <script src="~/Content/Admin/js/sparkline-chart.js"></script>
    <script src="~/Content/Admin/js/easy-pie-chart.js"></script>
    <script src="~/Content/Admin/js/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/Content/Admin/js/jquery-jvectormap-world-mill-en.js"></script>
    <script src="~/Content/Admin/js/xcharts.min.js"></script>
    <script src="~/Content/Admin/js/jquery.autosize.min.js"></script>
    <script src="~/Content/Admin/js/jquery.placeholder.min.js"></script>
    <script src="~/Content/Admin/js/gdp-data.js"></script>
    <script src="~/Content/Admin/js/morris.min.js"></script>
    <script src="~/Content/Admin/js/sparklines.js"></script>
    <script src="~/Content/Admin/js/charts.js"></script>
    <script src="~/Content/Admin/js/jquery.slimscroll.min.js"></script>
    <script>
        //knob
        $(function () {
            $(".knob").knob({
                'draw': function () {
                    $(this.i).val(this.cv + '%')
                }
            })
        });

        //carousel
        $(document).ready(function () {
            $("#owl-slider").owlCarousel({
                navigation: true,
                slideSpeed: 300,
                paginationSpeed: 400,
                singleItem: true

            });
        });

        //custom select box

        $(function () {
            $('select.styled').customSelect();
        });

        /* ---------- Map ---------- */
        $(function () {
            $('#map').vectorMap({
                map: 'world_mill_en',
                series: {
                    regions: [{
                        values: gdpData,
                        scale: ['#000', '#000'],
                        normalizeFunction: 'polynomial'
                    }]
                },
                backgroundColor: '#eef3f7',
                onLabelShow: function (e, el, code) {
                    el.html(el.html() + ' (GDP - ' + gdpData[code] + ')');
                }
            });
        });
    </script>

</body>

</html>
@RenderSection("scripts", required: false)